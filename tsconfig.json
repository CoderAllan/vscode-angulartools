{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2018",
		"outDir": "out",
		"lib": [
			"es2018"
		],
		"sourceMap": true,
		"rootDir": "src",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@commands": ["src/commands"],
      "@src": ["src"],
      "@model": ["src/model"]
    }
	},
	"exclude": [
		"node_modules",
		".vscode-test"
  ]
}
